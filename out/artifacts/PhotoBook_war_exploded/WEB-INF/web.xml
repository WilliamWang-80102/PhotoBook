<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
                                xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                                xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
                                version="4.0">
    <servlet>
        <servlet-name>FileDownLoadServlet</servlet-name>
        <servlet-class>com.whu.controller.download.FileDownLoadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileDownLoadServlet</servlet-name>
        <url-pattern>/FileDownLoadServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.whu.controller.login_register.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>com.whu.controller.login_register.RegisterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UploadTestServlet</servlet-name>
        <servlet-class>com.whu.controller.upload.UploadTestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadTestServlet</servlet-name>
        <url-pattern>/UploadTestServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SaveServlet</servlet-name>
        <servlet-class>com.whu.controller.SaveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SaveServlet</servlet-name>
        <url-pattern>/SaveServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>ModuleServlet</servlet-name>
        <servlet-class>com.whu.controller.ModuleServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ModuleServlet</servlet-name>
        <url-pattern>/ModuleServlet</url-pattern>
    </servlet-mapping>
</web-app>